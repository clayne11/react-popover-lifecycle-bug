{
  "name": "react-redux-starter-kit",
  "version": "0.16.0",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint ./src/**/*.js",
    "compile": "webpack",
    "start": "npm run dev",
    "dev": "NODE_ENV=development node --harmony bin/webpack-dev-server",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "node ./node_modules/karma/bin/karma start",
    "test:dev": "NODE_ENV=development npm run test -- --watch",
    "test:lint": "eslint ./tests/**/*.spec.js",
    "deploy": "npm run lint && npm run test && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
  "dependencies": {
    "babel": "^6.5.2",
    "dotenv": "^2.0.0",
    "history": "^4.4.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-popover": "^0.4.4",
    "react-redux": "^4.0.0",
    "react-router": "3.0.0",
    "redux": "^3.0.0",
    "redux-router": "^2.1.2",
    "redux-thunk": "^2.1.0",
    "yargs": "^6.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "autoprefixer-loader": "^3.1.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-runtime": "^6.18.0",
    "css-loader": "^0.26.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.2.0",
    "node-sass": "^3.3.3",
    "phantomjs": "^2.1.7",
    "phantomjs-polyfill": "0.0.2",
    "react-addons-test-utils": "^15.4.1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.4",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  }
}
